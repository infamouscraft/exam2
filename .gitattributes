# Auto detect text files and perform LF normalization
* text=auto
data = ["1234", "123456","12345678910", "1234567", "013", "011", "8543210", "12345", "1238974"]

listX = []
listY = []
def tuple_of_item(arg):
    x = 0
    while x < len(arg):
        if len(arg[x]) <= 4:
            listX.append(arg[x])
        elif len(arg[x]) >= 7:
            listY.append(arg[x])
        x = x+1
    z = (listX,listY)
    print(z)
    
tuple_of_item(data)

